<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="sample-parent"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <fileSets>
    <fileSet filtered="true" encoding="UTF-8">
      <directory />
    </fileSet>
  </fileSets>

  <requiredProperties>
    <requiredProperty key="libraryName">
      <defaultValue>my-project</defaultValue>
    </requiredProperty>

    <requiredProperty key="libraryDescription">
      <defaultValue>Does awesome things with JavaScript and CSS.</defaultValue>
    </requiredProperty>

    <requiredProperty key="libraryVersion">
      <defaultValue>1.0.0</defaultValue>
    </requiredProperty>

    <requiredProperty key="jsDisabled">
      <defaultValue>false</defaultValue>
    </requiredProperty>

    <requiredProperty key="jsClassifier">
      <defaultValue>min</defaultValue>
    </requiredProperty>

    <requiredProperty key="cssDisabled">
      <defaultValue>false</defaultValue>
    </requiredProperty>

    <requiredProperty key="cssClassifier">
      <defaultValue>min</defaultValue>
    </requiredProperty>

    <requiredProperty key="licenseName">
      <defaultValue>MIT</defaultValue>
    </requiredProperty>

    <requiredProperty key="licenseUrl">
      <defaultValue>https://opensource.org/licenses/MIT</defaultValue>
    </requiredProperty>
  </requiredProperties>
</archetype-descriptor>
